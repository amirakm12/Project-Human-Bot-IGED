{
  "system": {
    "name": "IGED",
    "version": "1.0.0",
    "description": "Project Human Bot - Sovereign AI Assistant",
    "offline_mode": false,
    "debug_mode": false
  },
  "voice": {
    "enabled": true,
    "sensitivity": 0.5,
    "timeout": 5,
    "phrase_timeout": 1.0,
    "language": "en-US",
    "model": "base"
  },
  "admin_panel": {
    "enabled": true,
    "host": "localhost",
    "port": 8080,
    "debug": false,
    "cors_enabled": true
  },
  "android_client": {
    "enabled": true,
    "host": "0.0.0.0",
    "port": 9090,
    "ssl_enabled": false
  },
  "encryption": {
    "enabled": true,
    "algorithm": "AES256",
    "key_file": "config/secret.key",
    "rotate_keys": false,
    "key_rotation_days": 30
  },
  "memory": {
    "enabled": true,
    "file_path": "memory/memory_log.json",
    "max_entries": 10000,
    "encryption": true,
    "backup_enabled": true,
    "backup_frequency": "daily"
  },
  "logging": {
    "level": "INFO",
    "file": "logs/iged.log",
    "max_size": 10485760,
    "backup_count": 5,
    "format": "%(asctime)s - %(name)s - %(levelname)s - %(message)s"
  },
  "agents": {
    "codegen": {
      "enabled": true,
      "output_dir": "output/codegen",
      "template_dir": "templates/",
      "timeout": 300
    },
    "data_miner": {
      "enabled": true,
      "output_dir": "output/data_analysis",
      "plot_dpi": 300,
      "plot_format": "png",
      "timeout": 600
    },
    "secops": {
      "enabled": true,
      "output_dir": "output/security",
      "scan_threads": 10,
      "timeout": 30
    },
    "network_intelligence": {
      "enabled": true,
      "output_dir": "output/network_intelligence",
      "scan_timeout": 60,
      "max_hosts": 254
    },
    "remote_control": {
      "enabled": true,
      "output_dir": "output/remote_control",
      "connection_timeout": 30
    }
  },
  "plugins": {
    "enabled": true,
    "directory": "plugins/",
    "timeout": 300,
    "sandbox": true,
    "auto_reload": true
  },
  "gui": {
    "enabled": true,
    "theme": "dark",
    "geometry": "1200x800",
    "position": [100, 100],
    "auto_save_layout": true
  },
  "performance": {
    "max_worker_threads": 10,
    "memory_limit_mb": 2048,
    "cpu_limit_percent": 80,
    "garbage_collect_frequency": 300
  },
  "security": {
    "rate_limiting": true,
    "max_requests_per_minute": 60,
    "require_auth": false,
    "session_timeout": 3600
  },
  "paths": {
    "config": "config/",
    "logs": "logs/",
    "memory": "memory/",
    "output": "output/",
    "plugins": "plugins/",
    "agents": "agents/",
    "ui": "ui/",
    "templates": "templates/"
  }
}